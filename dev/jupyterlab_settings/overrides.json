{
    "@jupyterlab/apputils-extension:themes": {
        "theme": "JupyterLab Horizon",
        "theme-scrollbars": true
    },
    "@jupyterlab/application-extension:sidebar": {
        "overrides": {
            "jp-property-inspector": "left",
            "jp-debugger-sidebar": "left"
        }
    },
    "@jupyterlab/fileeditor-extension:plugin": {
        "editorConfig": {
            "autoClosingBrackets": true,
            "cursorBlinkRate": 530,
            "insertSpaces": true,
            "lineNumbers": true,
            "lineWrap": "off",
            "matchBrackets": true,
            "tabSize": 4,
            "rulers": []
        }
    },
    "@jupyterlab/filebrowser-extension:browser": {
        "showHiddenFiles": true
    },
    "@jupyterlab/codemirror-extension:commands": {
        "styleActiveLine": true
    },
    "@ryantam626/jupyterlab_code_formatter:settings": {
        "preferences": {
            "default_formatter": {
                "python": [
                    "black"
                ]
            }
        },
        "black": {
            "line_length": 88,
            "string_normalization": true
        },
        "formatOnSave": false,
        "suppressFormatterErrors": false
    },
    "@jupyterlab/shortcuts-extension:shortcuts": {
        "shortcuts": [
            {
                "command": "jupyterlab_code_formatter:format",
                "keys": [
                    "Accel Shift F"
                ],
                "selector": ".jp-Notebook.jp-mod-editMode"
            }
        ]
    }
}